<?xml version="1.0" encoding="utf-8" ?>

<model xmlns="http://www.top-logic.com/ns/dynamic-types/6.0">
	<module name="Artworks">
		<interface name="Manuscript">
			<generalizations>
				<generalization type="research:DataItem"/>
			</generalizations>
			<attributes>
				<property name="year"
					type="tl.core:Integer"
				/>
				<property name="title"
					type="tl.core:String"
				/>
				<reference name="authors"
					inverse-reference="manuscripts"
					kind="forwards"
					multiple="true"
					navigate="true"
					ordered="true"
					type="Humans:Author"
				/>
			</attributes>
		</interface>
		<class name="Article">
			<generalizations>
				<generalization type="Manuscript"/>
			</generalizations>
			<attributes>
				<property name="abstract"
					type="tl.core:String"
				/>
				<property name="doi"
					type="tl.core:String"
				/>
				<reference name="journal"
					kind="forwards"
					navigate="true"
					type="Humans:Journal"
				/>
				<property name="number"
					type="tl.core:String"
				/>
				<property name="pages"
					type="tl.core:String"
				/>
				<property name="volume"
					type="tl.core:String"
				/>
			</attributes>
		</class>
		<class name="Book">
			<generalizations>
				<generalization type="Manuscript"/>
			</generalizations>
			<attributes>
				<property name="address"
					type="tl.core:String"
				/>
				<property name="edition"
					type="tl.core:String"
				/>
				<property name="publisher"
					type="tl.core:String"
				/>
			</attributes>
		</class>
	</module>
</model>