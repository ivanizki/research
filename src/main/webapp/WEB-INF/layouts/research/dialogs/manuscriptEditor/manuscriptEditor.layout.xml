						fullQualifiedName="Artworks:Article#abstract"
						type="tl.core:String"
					/>
					<field
						attribute="doi"
						fullQualifiedName="Artworks:Article#doi"
						type="tl.core:String"
					/>
					<field
						attribute="journal"
						fullQualifiedName="Artworks:Article#journal"
						type="Humans:Journal"
					/>
					<field
						attribute="volume"
						fullQualifiedName="Artworks:Article#volume"
						type="tl.core:String"
					/>
					<field
						attribute="number"
						fullQualifiedName="Artworks:Article#number"
						type="tl.core:String"
					/>
				</formDefinition>
			</form>
		</forms>
	</arguments>
</config:template-call>